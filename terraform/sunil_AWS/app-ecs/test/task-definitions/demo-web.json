    "environment": [
      {
        "name": "APP_DEBUG",
        "value": "true"
      },
      {
        "name": "APP_ENV",
        "value": "production"
      },
      {
        "name": "APP_NAME",
        "value": "demo-web"
      }
    ]
  }
]